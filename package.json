{
  "name": "cwrc-writer-base",
  "version": "5.1.6",
  "description": "intuitive document editing with xml and rdf",
  "keywords": [
    "xml",
    "rdf",
    "editor",
    "annotation"
  ],
  "homepage": "https://github.com/cwrc/CWRC-WriterBase#readme",
  "bugs": {
    "url": "https://github.com/cwrc/CWRC-WriterBase/issues"
  },
  "license": "GPL-2.0",
  "author": "Andrew MacDonald <andrewjames.code@gmail.com>",
  "contributors": [
    "James Chartrand <jc.chartrand@gmail.com>",
    "Luciano Frizzera <lucaju@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cwrc/CWRC-WriterBase.git"
  },
  "main": "src/js/writer.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "npm run build-editor-css",
    "build-editor-css": "lessc src/css/editor.less src/css/build/editor.css",
    "build-editor-css-dev": "less-watch-compiler src/css src/css/build editor.less",
    "cm": "git-cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "report-coverage": "cat ./coverage/coverage-final.json | codecov",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "build-dependency-graph": "depcruise --include-only \"^src\" --output-type dot src | dot -T svg | depcruise-wrap-stream-in-html > ./dependency-graph/dependency-graph.html"
  },
  "browser": {
    "objtree": "./src/lib/objtree/ObjTree.js",
    "jquery-watermark": "./src/lib/jquery/jquery.watermark.min.js",
    "jquery-popup": "./src/lib/jquery/jquery.popup.js",
    "layout": "./src/lib/jquery/jquery.layout_and_plugins.js",
    "dialogForm": "./src/js/dialogs/dialogForm/dialogForm.js",
    "annotationsManager": "./src/js/entities/annotationsManager.js",
    "entity": "./src/js/entities/entity.js",
    "mapper": "./src/js/schema/mapper.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "css": "^3.0.0",
    "fscreen": "1.0.2",
    "iso-639-2": "^2.0.0",
    "jquery": "3.4.1",
    "jquery-contextmenu": "2.9.0",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.2.1",
    "jstree": "3.3.8",
    "moment": "^2.27.0",
    "openseadragon": "^2.4.1",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tinymce": "4.9.8"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@jest-runner/electron": "^3.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.7.0",
    "babel-jest": "^25.5.1",
    "codecov": "^3.7.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "electron": "^9.1.0",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "fetch-mock": "^9.10.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.11.3",
    "less-watch-compiler": "^1.14.6",
    "semantic-release": "^17.1.1",
    "travis-deploy-once": "^5.0.11"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
}
